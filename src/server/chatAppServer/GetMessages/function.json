{
  "disabled": false,
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "route": "messages/{group_name}",
      "methods": [
        "get"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name": "cosmosDBMessage",
      "type": "cosmosDB",
      "databaseName": "CowayDevDb",
      "collectionName": "chat",
      "sqlQuery": "select * from c where c.groupName = {group_name} order by c._ts desc offset 0 limit 500",
      "connectionStringSetting": "AzureWebJobsCosmosDBConnectionString",
      "direction": "in"
    }
  ]
}
